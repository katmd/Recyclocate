{
  "expo": {
    "name": "Recyclocate",
    "slug": "recyclocate",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.katmd.recyclocate"
    },
    "android": {
      "package": "com.katmd.recyclocate",
      "config": {
        "googleMaps": {"apiKey": "REACT_APP_GOOGLE_MAPS_API_KEY"}
      }
    }
  }
}
